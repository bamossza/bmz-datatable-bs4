{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "bamossza-lib-app": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                "@schematics/angular:component": {
                    "styleext": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/bamossza-lib-app",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/datatables.net-bs4/css/dataTables.bootstrap4.css",
                            "node_modules/datatables.net-fixedcolumns-bs4/css/fixedColumns.bootstrap4.css",
                            "node_modules/datatables.net-rowgroup-bs4/css/rowGroup.bootstrap4.css",
                            "node_modules/datatables.net-fixedheader-bs4/css/fixedHeader.bootstrap4.css"
                        ],
                        "scripts": [
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/datatables.net/js/jquery.dataTables.js",
                            "node_modules/datatables.net-bs4/js/dataTables.bootstrap4.js",
                            "node_modules/datatables.net-fixedcolumns/js/dataTables.fixedColumns.js",
                            "node_modules/datatables.net-fixedcolumns-bs4/js/fixedColumns.bootstrap4.js",
                            "node_modules/datatables.net-fixedheader/js/dataTables.fixedHeader.js",
                            "node_modules/datatables.net-fixedheader-bs4/js/fixedHeader.bootstrap4.js",
                            "node_modules/datatables.net-rowgroup/js/dataTables.rowGroup.js",
                            "node_modules/datatables.net-rowgroup-bs4/js/rowGroup.bootstrap4.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "bamossza-lib-app:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "bamossza-lib-app:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "bamossza-lib-app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "bamossza-lib-app-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "bamossza-lib-app:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "bamossza-lib-app:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "bmz-datepicker": {
            "root": "projects/bmz-datepicker",
            "sourceRoot": "projects/bmz-datepicker/src",
            "projectType": "library",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/bmz-datepicker/tsconfig.lib.json",
                        "project": "projects/bmz-datepicker/ng-package.json"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/bmz-datepicker/src/test.ts",
                        "tsConfig": "projects/bmz-datepicker/tsconfig.spec.json",
                        "karmaConfig": "projects/bmz-datepicker/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/bmz-datepicker/tsconfig.lib.json",
                            "projects/bmz-datepicker/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "bmz-utils": {
            "root": "projects/bmz-utils",
            "sourceRoot": "projects/bmz-utils/src",
            "projectType": "library",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/bmz-utils/tsconfig.lib.json",
                        "project": "projects/bmz-utils/ng-package.json"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/bmz-utils/src/test.ts",
                        "tsConfig": "projects/bmz-utils/tsconfig.spec.json",
                        "karmaConfig": "projects/bmz-utils/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/bmz-utils/tsconfig.lib.json",
                            "projects/bmz-utils/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "bmz-datatable-bs4": {
            "root": "projects/bmz-datatable-bs4",
            "sourceRoot": "projects/bmz-datatable-bs4/src",
            "projectType": "library",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/bmz-datatable-bs4/tsconfig.lib.json",
                        "project": "projects/bmz-datatable-bs4/ng-package.json"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/bmz-datatable-bs4/src/test.ts",
                        "tsConfig": "projects/bmz-datatable-bs4/tsconfig.spec.json",
                        "karmaConfig": "projects/bmz-datatable-bs4/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/bmz-datatable-bs4/tsconfig.lib.json",
                            "projects/bmz-datatable-bs4/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "bamossza-lib-app"
}
